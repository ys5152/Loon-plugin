# 防止 DNS 走系统通道
[Plugin]
http-dns-block = type=http-response,behavior=plugin,script-path=https://raw.githubusercontent.com/lxhao61/integrated-script/master/dns/Block_HTTPDNS.plugin,timeout=10

dns-leak-prevent = type=http-response,behavior=plugin,script-path=https://raw.githubusercontent.com/lxhao61/integrated-script/master/dns/Prevent_DNS_Leaks.plugin,timeout=10
